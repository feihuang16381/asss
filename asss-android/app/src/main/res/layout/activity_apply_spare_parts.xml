<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/windowBackground"
    tools:context="com.cqut.icode.asss_android.activity.ApplySparePartsActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/spare_parts"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/guideline2"
        tools:text="@string/spare_parts" />

    <EditText
        android:id="@+id/spare_parts"
        android:layout_width="218dp"
        android:layout_height="30dp"
        android:background="@drawable/et_selector"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingLeft="5dp"
        android:textSize="15sp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintTop_toTopOf="@+id/guideline2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/apply_date"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/guideline4"
        tools:text="@string/apply_date" />

    <EditText
        android:id="@+id/apply_date"
        android:layout_width="103dp"
        android:layout_height="30dp"
        android:background="@drawable/et_selector"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingLeft="5dp"
        android:textSize="15sp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintTop_toTopOf="@+id/guideline4" />

    <EditText
        android:id="@+id/connect_project"
        android:layout_width="218dp"
        android:layout_height="30dp"
        android:background="@drawable/et_selector"
        android:ems="10"
        android:inputType="textPersonName"
        android:paddingLeft="5dp"
        android:textSize="15sp"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="16dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="16dp" />

    <EditText
        android:id="@+id/remark"
        android:layout_width="218dp"
        android:layout_height="210dp"
        android:textSize="15sp"
        android:paddingLeft="5dp"
        android:background="@drawable/et_selector"
        android:gravity="top"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintTop_toTopOf="@+id/guideline6" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/connect_project"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/guideline5"
        tools:text="@string/connect_project" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="134dp"
        tools:layout_editor_absoluteY="134dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline3"
        android:orientation="vertical"
        app:layout_constraintGuide_end="268dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="92dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline4"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="172dp"
        tools:layout_editor_absoluteY="172dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline5"
        app:layout_constraintGuide_begin="248dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="248dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/remark"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/guideline6"
        tools:text="@string/remark" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline6"
        app:layout_constraintGuide_begin="292dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="292dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline7"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="94dp"
        tools:layout_editor_absoluteX="0dp"
        app:layout_constraintGuide_begin="94dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/apply_name"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/guideline7"
        tools:text="@string/apply_name" />

    <EditText
        android:id="@+id/apply_name"
        android:layout_width="218dp"
        android:layout_height="30dp"
        android:paddingLeft="5dp"
        android:background="@drawable/et_selector"
        android:ems="10"
        android:textSize="15sp"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintTop_toTopOf="@+id/guideline7" />

    <FrameLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1">

        <include layout="@layout/the_theme_toolbar" />

        <TextView
            android:id="@+id/tv_affirm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="end|center_vertical"
            android:layout_marginRight="20dp"
            android:text="@string/affirm"
            android:textColor="@color/colorAccent"
            android:textSize="18sp" />
    </FrameLayout>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline10"
        app:layout_constraintGuide_begin="211dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="211dp"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/apply_time"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/guideline10"
        tools:text="@string/apply_time" />

    <EditText
        android:id="@+id/apply_time"
        android:layout_width="103dp"
        android:layout_height="30dp"
        android:paddingLeft="5dp"
        android:textSize="15sp"
        android:background="@drawable/et_selector"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintLeft_toLeftOf="@+id/guideline3"
        app:layout_constraintTop_toTopOf="@+id/guideline10" />


</android.support.constraint.ConstraintLayout>
